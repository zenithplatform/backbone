				}
			},
			"handlers": {
				"console": {
					"class": "logging.StreamHandler",
					"level": "DEBUG",
					"formatter": "simple",
					"stream": "ext://sys.stdout"
				},
				"info_file_handler": {
					"class": "logging.handlers.RotatingFileHandler",
					"level": "INFO",
					"formatter": "simple",
					"filename": "D:\\Programming\\Astronomy\\Dev\\ZenithLogs\\backbone_local_info.log",
					"maxBytes": 10485760,
					"backupCount": 20,
					"encoding": "utf8"
				},
				"error_file_handler": {
					"class": "logging.handlers.RotatingFileHandler",
					"level": "ERROR",
					"formatter": "simple",
					"filename": "D:\\Programming\\Astronomy\\Dev\\ZenithLogs\\backbone_local_errors.log",
					"maxBytes": 10485760,
					"backupCount": 20,
					"encoding": "utf8"
				}
			},
			"loggers": {
				"local.infrastructure.pipes": {
					"level": "INFO",
					"handlers": ["info_file_handler", "error_file_handler"],
					"propagate": "no"
				}
			},
			"root": {
				"level": "INFO",
				"handlers": ["console"]
			}
		}
	}
