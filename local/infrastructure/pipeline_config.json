						"exit": false
					},
					"input": {
						"port": 18802,
						"type": "inbound",
						"kind": "pull",
						"multiplex": true,
						"entry": false,
						"exit": false
					},
					"callback": {
						"port": 18803,
						"type": "inbound",
						"kind": "pull",
						"multiplex": true,
						"entry": false,
						"exit": false
					}
				}
